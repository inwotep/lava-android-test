import os
import sys

BINDIR=os.path.abspath(os.path.dirname(os.path.realpath(sys.argv[0])))
ROOT=os.path.dirname(BINDIR)
TEST_DIR=os.path.join(ROOT,'lava_android_test')
if os.path.exists(TEST_DIR) and ROOT not in sys.path:
    sys.path.insert(0, ROOT)

import lava_android_test.main

if __name__ == '__main__':
    exit_code = lava_android_test.main.main(sys.argv)
    sys.exit(exit_code)
